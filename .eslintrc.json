{
	"extends": "airbnb-base",
	"env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "rules": {
        "indent": [
            "error",
            "tab"
        ],
        "linebreak-style": [
            "error",
            "unix"
        ],
		"no-console": "off",
		"no-tabs": "off"
    }
}